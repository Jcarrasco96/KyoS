@model KyoS.Web.Models.ClientViewModel

<div class="col-md-12">
    &nbsp;
</div>
<div class="col-md-push-12">
    <div class="form-group col-md-3">
        <label asp-for="EmergencyContact.Name" class="control-label">Emergency Contact Name</label>
        <input asp-for="EmergencyContact.Name" class="form-control" disabled />
    </div>
    <div class="form-group col-md-2">
        <label asp-for="IdRelationshipEC" class="control-label"></label>
        <select asp-for="IdRelationshipEC" asp-items="Model.RelationshipsEC" class="form-control" disabled></select>
    </div>
    <div class="form-group col-md-3">
        <label asp-for="EmergencyContact.Address" class="control-label">Address</label>
        <input asp-for="EmergencyContact.Address" class="form-control" disabled />
    </div>
    <div class="form-group col-md-2">
        <label asp-for="EmergencyContact.Country" class="control-label">Country</label>
        <input asp-for="EmergencyContact.Country" class="form-control" disabled />
    </div>
    <div class="form-group col-md-2">
        <label asp-for="EmergencyContact.City" class="control-label">City</label>
        <input asp-for="EmergencyContact.City" class="form-control" disabled />
    </div>
</div>
<div class="col-md-push-12">
    <div class="form-group col-md-2">
        <label asp-for="EmergencyContact.State" class="control-label">State</label>
        <input asp-for="EmergencyContact.State" class="form-control" disabled />
    </div>
    <div class="form-group col-md-1">
        <label asp-for="EmergencyContact.ZipCode" class="control-label">Zip Code</label>
        <input asp-for="EmergencyContact.ZipCode" class="form-control" disabled />
    </div>
    <div class="form-group col-md-3">
        <label asp-for="EmergencyContact.Address" class="control-label">Address Line 2</label>
        <input asp-for="EmergencyContact.Address" class="form-control" disabled />
    </div>
    <div class="form-group col-md-2">
        <label asp-for="EmergencyContact.Telephone" class="control-label">Telephone</label>
        <input asp-for="EmergencyContact.Telephone" class="form-control" disabled />
    </div>
    <div class="form-group col-md-2">
        <label asp-for="EmergencyContact.TelephoneSecondary" class="control-label">Telephone Secundary</label>
        <input asp-for="EmergencyContact.TelephoneSecondary" class="form-control" disabled />
    </div>
    <div class="form-group col-md-2">
        <label asp-for="EmergencyContact.Email" class="control-label">Email</label>
        <input asp-for="EmergencyContact.Email" class="form-control" disabled />
    </div>
</div>