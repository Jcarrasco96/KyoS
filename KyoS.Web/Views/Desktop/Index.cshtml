            <div class="col-md-2">
                @if (ViewBag.NotesPending == "0")
                {
                    <div class="thumbnail list-group-item list-group-item-text">
                        <h4 style="align-content:end;"><i class="icon-share" style="padding-right: 10px !important;"></i>Pending<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Notes</h4>
                        <span class="message-counter-cero">@ViewBag.NotesPending</span>
                    </div>
                }
                else
                {
                    <a style="text-decoration: none;" asp-controller="TCMNotes" asp-action="NotesStatus" asp-route-status="@KyoS.Common.Enums.NoteStatus.Pending" role="button">
                        <div class="thumbnail list-group-item list-group-item-info">
                            <h4 style="align-content:end;"><i class="icon-share" style="padding-right: 10px !important;"></i>Pending<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Notes</h4>
                            <span class="message-counter-info">@ViewBag.NotesPending</span>
                        </div>
                    </a>
                }
            </div>
            <div class="col-md-2">
                @if (ViewBag.NotesApproved == "0")
                {
                    <div class="thumbnail list-group-item list-group-item-text">
                        <h4 style="align-content:end;"><i class="icon-file-alt" style="padding-right: 10px !important;"></i>Approved<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Notes</h4>
                        <span class="message-counter-cero">@ViewBag.ServicePlanReviewPending</span>
                    </div>
                }
                else
                {
                    <a style="text-decoration: none;" asp-controller="TCMNotes" asp-action="NotesStatus" asp-route-status="@KyoS.Common.Enums.NoteStatus.Approved" role="button">
                        <div class="thumbnail list-group-item list-group-item-success">
                            <h4 style="align-content:end;"><i class="icon-file-alt" style="padding-right: 10px !important;"></i>Approved<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Notes</h4>
                            <span class="message-counter-success">@ViewBag.NotesApproved</span>
                        </div>
                    </a>
                }
            </div>
            
        }
    </div>
</div>
