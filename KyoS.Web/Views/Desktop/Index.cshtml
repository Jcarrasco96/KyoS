                            <h4 style="align-content:end;"><i class="icon-refresh" style="padding-right: 10px !important;"></i>Cases without<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Discharge</h4>
                            <span class="message-counter">@ViewBag.ClientWithoutDischarge</span>
                        </div>
                    </a>
                }
            </div>
            <div class="col-md-2">
                @if (ViewBag.NotesApproved == 0)
                {
                    <div class="thumbnail list-group-item list-group-item-text">
                        <h4 style="align-content:end;"><i class="icon-file-text" style="padding-right: 10px !important;"></i>Approved<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Notes</h4>
                        <span class="message-counter-cero">@ViewBag.ServicePlanReviewPending</span>
                    </div>
                }
                else
                {
                    <a style="text-decoration: none;" asp-controller="TCMNotes" asp-action="NotesStatus" asp-route-status="@KyoS.Common.Enums.NoteStatus.Approved" role="button">
                        <div class="thumbnail list-group-item list-group-item-success">
                            <h4 style="align-content:end;"><i class="icon-file-text" style="padding-right: 10px !important;"></i>Approved<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Notes</h4>
                            <span class="message-counter-success">@ViewBag.NotesApproved</span>
                        </div>
                    </a>
                }
            </div>
            <div class="col-md-3">
            </div>
        }
        @if (User.IsInRole("Biller"))
        {
            <div class="col-md-12">
                &nbsp;
            </div>
            <div class="col-md-12">
                <div class="col-md-3">
                </div>
                <div class="col-md-6">
                    @*<h4 style="text-align:center;display: block;font-size: 18px;color: #888da8;max-width: 100%;">@user.Clinic.Name</h4>*@
                    <h5 style="text-align:center;display: block;font-size: 15px;color: #b4b7c8;max-width: 100%;">@DateTime.Now.ToLongDateString()</h5>
                </div>
                <div class="col-md-3">
                </div>
            </div>
            <div class="col-md-12">
                &nbsp;
            </div>
            <div class="col-md-4">
                &nbsp;
            </div>
            <div class="col-md-2">
                <a style="text-decoration: none;" asp-controller="Notes" asp-action="BillingReport1" role="button">
                    <div class="thumbnail list-group-item list-group-item-success">
                        <h4 style="align-content:end;"><i class="icon-usd" style="padding-right: 10px !important;"></i>CMH Billing<br>&nbsp;&nbsp;&nbsp;&nbsp;Report</h4>
                        <span class="message-counter-success">$</span>
                    </div>
                </a>
            </div>
             <div class="col-md-2">
                    <a style="text-decoration: none;" asp-controller="TCMBilling" asp-action="BillingForWeek" role="button">
                        <div class="thumbnail list-group-item list-group-item-success">
                            <h4 style="align-content:end;"><i class="icon-dollar" style="padding-right: 10px !important;"></i>TCM Billing<br>&nbsp;&nbsp;&nbsp;&nbsp;Report</h4>
                            <span class="message-counter-success">$</span>
                        </div>
                    </a>
                </div>
            <div class="col-md-4">
                &nbsp;
            </div>
           

        }
    </div>
</div>
