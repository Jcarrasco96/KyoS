@model KyoS.Web.Models.SettingManagerViewModel
<div class="col-md-push-12">
    &nbsp;
</div>
<div class="col-md-push-12">
    <div class="form-group col-md-2">
        <label asp-for="TCMLockCreateNote" class="control-label">Create notes until </label>
        <input asp-for="TCMLockCreateNote" class="form-control" disabled />
        <span asp-validation-for="TCMLockCreateNote" class="text-danger"></span>
    </div>
    <div class="form-group col-md-2">
        <label asp-for="TCMInitialTime" class="control-label">Start Time *</label>
        <input asp-for="TCMInitialTime" class="form-control" />
        <span asp-validation-for="TCMInitialTime" class="text-danger"></span>
    </div>
    <div class="form-group col-md-2">
        <label asp-for="TCMEndTime" class="control-label">End Time *</label>
        <input asp-for="TCMEndTime" class="form-control" />
        <span asp-validation-for="TCMEndTime" class="text-danger"></span>
    </div>
    <div class="form-group col-md-2">
        <label asp-for="UnitsForDayForClient" class="control-label">Max. daily units per client *</label>
        <input asp-for="UnitsForDayForClient" class="form-control" />
        <span asp-validation-for="UnitsForDayForClient" class="text-danger"></span>
    </div>
    <div class="form-group col-md-2">
        <label asp-for="IdFiltroPayStub" class="control-label">Select the filter for Paystub.</label>
        <select asp-for="IdFiltroPayStub" asp-items="Model.FiltroPayStubs" class="form-control"></select>
        <span asp-validation-for="IdFiltroPayStub" class="text-danger"></span>
    </div>
    <div class="form-group col-md-6">
        <hr />
    </div>
</div>
<div class="col-md-push-12">
    <div class="form-group col-md-12">
        <input type="checkbox" asp-for="LockTCMNoteForUnits" class="form-check-input"> Unable create notes for unavaliable units<br />
    </div>
</div>
<div class="col-md-push-12">
    <div class="form-group col-md-12">
        <input type="checkbox" asp-for="TCMSupervisorEdit" class="form-check-input"> Enable edit documents by TCM supervisor<br />
    </div>
</div>
<div class="col-md-push-12">
    <div class="form-group col-md-12">
        <input type="checkbox" asp-for="CreateNotesTCMWithServiceplanInEdition" class="form-check-input"> Enable create notes with Services Plan in edition<br />
    </div>
</div>
<div class="col-md-push-12">
    <div class="form-group col-md-12">
        <input type="checkbox" asp-for="TCMSupervisionTimeWithCaseManager" class="form-check-input"> Supervision time in the presence of the TCM<br />
    </div>
</div>
<div class="col-md-push-12">
    <div class="form-group col-md-12">
        <input type="checkbox" asp-for="CreateTCMNotesWithoutDomain" class="form-check-input"> Create initials notes without Service Plan Domains<br />
    </div>
</div>
<div class="col-md-push-12">
    <div class="form-group col-md-12">
        <input type="checkbox" asp-for="LockTCMNoteForOneMonthIdle" class="form-check-input"> Unable create TCM notes for idle cases (1 month)<br />
    </div>
</div>
<div class="col-md-push-12">
    <div class="form-group col-md-12">
        <input type="checkbox" asp-for="LockTCMNoteForDx" class="form-check-input"> Unable create TCM notes without a valid diagnosis<br />
    </div>
</div>
<div class="col-md-push-12">
    <div class="form-group col-md-12">
        <input type="checkbox" asp-for="LockTCMNoteForAuth" class="form-check-input"> Unable create TCM notes without a valid authorization<br />
    </div>
</div>




