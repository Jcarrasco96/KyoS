@model KyoS.Web.Models.IntakeConsentForTreatmentViewModel
@{
    ViewData["Title"] = "Create Intake Consent For Treatment";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>CREATE CONSENT FOR TREATMENT AND RECEIVE MEDICATION</title>
</head>
<body>

    <div class="breadcrumbs" id="breadcrumbs">
        <ul class="breadcrumb">
            <li>
                <i class="icon-home"></i>
                @Html.ActionLink("Intakes", "Index", "Intakes")
            </li>
            <li class="active">Create Intake Consent for Treatment and Receive Medication</li>
        </ul><!--.breadcrumb-->
    </div>

    <h4>Create Intake Consent for Treatment and Receive Medication</h4>
    <hr />
    <div class="row">

        <div>
            <form asp-action="CreateConsentForTreatment" asp-route-id="@Model.Id" enctype="multipart/form-data">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <input type="hidden" asp-for="IdClient" />
                <input type="hidden" asp-for="Client_FK" />
                <input type="hidden" asp-for="AdmissionedFor" />
                <partial name="_IntakesConsentForTreatment" />
                <div class="form-group col-md-12">
                    <input type="submit" value="Save" class="btn btn-primary" />
                    <a asp-action="IntakeDashboard" asp-route-id="@Model.Client.Id" class="btn btn-success">Back</a>
                </div>
            </form>
        </div>
    </div>
</body>
</html>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    <script src="~/js/jquery.dataTables.min.js"></script>

    <script type="text/javascript">

    </script>
}


