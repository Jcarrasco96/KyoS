@model KyoS.Web.Models.IntakeMedicalHistoryViewModel
<div class="col-md-12">
    <pre>
    <strong>MEDICAL HISTORY</strong>

        Recipient: <strong>@Html.DisplayFor(modelItem => Model.Client.Clinic.Name)</strong>                                                 Case No: <strong>@Html.DisplayFor(modelItem => Model.Client.Code)</strong>
            
        Primary Care Physician: <input asp-for="PrimaryCarePhysician" style="width:20%" disabled>
            
        Address Physician:      <input asp-for="AddressPhysician" style="width:25%" disabled>
            
        City:                   <input asp-for="City" style="width:20%" disabled>                           State: <input asp-for="State" disabled>                Zip:  <input asp-for="ZipCode" disabled>

        Start time   <input asp-for="StartTime" >                      End time   <input asp-for="EndTime" >
    </pre>
</div>
<div class="col-md-12">
    <pre>
    <strong>SECTION I.</strong> Select YES or No next to any medical problem that you currently have or have had in the past. If you do not understand an item, please ask for assistance.
    
    <strong>YES</strong>                                        <strong>YES</strong>                                          <strong>YES</strong>                                            
    <input type="checkbox" asp-for="Diphtheria" class="form-check-input"> Diphtheria                               <input type="checkbox" asp-for="StomachPain" class="form-check-input"> Stomach Pain                              <input type="checkbox" asp-for="FrequentColds" class="form-check-input"> Frequent Colds
    <input type="checkbox" asp-for="Mumps" class="form-check-input"> Mumps                                    <input type="checkbox" asp-for="BlackStools" class="form-check-input"> Black Stools                              <input type="checkbox" asp-for="HeartPalpitation" class="form-check-input"> Heart Palpitation
    <input type="checkbox" asp-for="Poliomyelitis" class="form-check-input"> Poliomyelitis                            <input type="checkbox" asp-for="NightSweats" class="form-check-input"> Night Sweats                              <input type="checkbox" asp-for="ChestPain" class="form-check-input"> Chest Pain
    <input type="checkbox" asp-for="RheumaticFever" class="form-check-input"> Rheumatic Fever                          <input type="checkbox" asp-for="FrequentVomiting" class="form-check-input"> Frequent Vomiting                         <input type="checkbox" asp-for="ShortnessOfBreath" class="form-check-input"> Shortness Of Breath
    <input type="checkbox" asp-for="WhoopingCough" class="form-check-input"> Whooping Cough                           <input type="checkbox" asp-for="SkinTrouble" class="form-check-input"> Skin Trouble                              <input type="checkbox" asp-for="SwellingOfFeet" class="form-check-input"> Swelling Of Feet
    <input type="checkbox" asp-for="Tuberculosis" class="form-check-input"> Tuberculosis                             <input type="checkbox" asp-for="PainfulMuscles" class="form-check-input"> Painful Muscles                           <input type="checkbox" asp-for="SwollenAnkles" class="form-check-input"> Swollen Ankles
    <input type="checkbox" asp-for="ScarletFever" class="form-check-input"> Scarlet Fever                            <input type="checkbox" asp-for="PainfulJoints" class="form-check-input"> Painful Joints                            <input type="checkbox" asp-for="ChronicIndigestion" class="form-check-input"> Chronic Indigestion
    <input type="checkbox" asp-for="Hepatitis" class="form-check-input"> Hepatitis                                <input type="checkbox" asp-for="BackPain" class="form-check-input"> Back Pain                                 <input type="checkbox" asp-for="VomitingOfBlood" class="form-check-input"> Vomiting Of Blood
    <input type="checkbox" asp-for="HighBloodPressure" class="form-check-input"> High Blood Pressure                      <input type="checkbox" asp-for="SeriousInjury" class="form-check-input"> Serious Injury                            <input type="checkbox" asp-for="Jaundice" class="form-check-input"> Jaundice (Yellowing of Skin)                        
    <input type="checkbox" asp-for="KidneyTrouble" class="form-check-input"> Kidney Trouble                           <input type="checkbox" asp-for="Surgery" class="form-check-input"> Surgery                                   <input type="checkbox" asp-for="Constipation" class="form-check-input"> Constipation
    <input type="checkbox" asp-for="KidneyStones" class="form-check-input"> Kidney Stones                            <input type="checkbox" asp-for="Arthritis" class="form-check-input"> Arthritis                                 <input type="checkbox" asp-for="BloodyStools" class="form-check-input"> Bloody Stools
    <input type="checkbox" asp-for="BloodInUrine" class="form-check-input"> Blood in Urine                           <input type="checkbox" asp-for="Hemorrhoids" class="form-check-input"> Hemorrhoids                               <input type="checkbox" asp-for="Cancer" class="form-check-input"> Cancer
    <input type="checkbox" asp-for="BurningUrine" class="form-check-input"> Burning Urine                            <input type="checkbox" asp-for="WeightLoss" class="form-check-input"> WeightLoss                                <input type="checkbox" asp-for="Diabetes" class="form-check-input"> Diabetes
    <input type="checkbox" asp-for="PainfulUrination" class="form-check-input"> Painful Urination                        <input type="checkbox" asp-for="FrequentHeadaches" class="form-check-input"> Frequent Headaches                        <input type="checkbox" asp-for="HayFever" class="form-check-input"> HayFever
    <input type="checkbox" asp-for="EyeTrouble" class="form-check-input"> Eye Trouble                              <input type="checkbox" asp-for="Fainting" class="form-check-input"> Fainting                                  <input type="checkbox" asp-for="Hernia" class="form-check-input"> Hernia
    <input type="checkbox" asp-for="HearingTrouble" class="form-check-input"> Hearing Trouble                          <input type="checkbox" asp-for="ConvulsionsOrFits" class="form-check-input"> Convulsions Or Fits                       <input type="checkbox" asp-for="HeadInjury" class="form-check-input"> Head Injury
    <input type="checkbox" asp-for="Fractures" class="form-check-input"> Fractures                                <input type="checkbox" asp-for="LossOfMemory" class="form-check-input"> Loss Of Memory                            <input type="checkbox" asp-for="Rheumatism" class="form-check-input"> Rheumatism
    <input type="checkbox" asp-for="EarInfections" class="form-check-input"> Ear Infections                           <input type="checkbox" asp-for="Nervousness" class="form-check-input"> Nervousness                               <input type="checkbox" asp-for="Epilepsy" class="form-check-input"> Epilepsy
    <input type="checkbox" asp-for="FrequentNoseBleeds" class="form-check-input"> Frequent Nose Bleeds                     <input type="checkbox" asp-for="ChronicCough" class="form-check-input"> Chronic Cough                             <input type="checkbox" asp-for="VaricoseVeins" class="form-check-input"> VaricoseVeins
    <input type="checkbox" asp-for="FrequentSoreThroat" class="form-check-input"> Frequent Sore Throat                     <input type="checkbox" asp-for="CoughingOfBlood" class="form-check-input"> Coughing Of Blood                         <input type="checkbox" asp-for="Anemia" class="form-check-input"> Anemia
    <input type="checkbox" asp-for="Hoarseness" class="form-check-input"> Hoarseness                               <input type="checkbox" asp-for="VenerealDisease" class="form-check-input"> VenerealDisease                           <input type="checkbox" asp-for="InfectiousDisease" class="form-check-input"> Infectious Disease
    
    <input type="checkbox" asp-for="Allergies" class="form-check-input"> Allergies  (including medications) If Yes, describe:   <input asp-for="Allergies_Describe" style="width:40%">
    </pre>
</div>
<div class="col-md-12">
    <pre>
    <strong>SECTION II.</strong> Has anyone in your family had or been treated for:

    <strong>YES</strong>                                                    <strong>Who? (Father, mother, sibling, grandparent, etc.)</strong>
    <input type="checkbox" asp-for="FamilyDiabetes" class="form-check-input"> Diabetes                               <input asp-for="FamilyDiabetes_" style="width:40%">
 
    <input type="checkbox" asp-for="FamilyCancer" class="form-check-input"> Cancer                                 <input asp-for="FamilyCancer_" style="width:40%">
 
    <input type="checkbox" asp-for="FamilyTuberculosis" class="form-check-input"> Tuberculosis                           <input asp-for="FamilyTuberculosis_" style="width:40%">
                              
    <input type="checkbox" asp-for="FamilyHeartDisease" class="form-check-input"> Heart Disease                          <input asp-for="FamilyHeartDisease_" style="width:40%">
                 
    <input type="checkbox" asp-for="FamilyKidneyDisease" class="form-check-input"> Kidney Disease                         <input asp-for="FamilyKidneyDisease_" style="width:40%">
                      
    <input type="checkbox" asp-for="FamilyHighBloodPressure" class="form-check-input"> High Blood Pressure                    <input asp-for="FamilyHighBloodPressure_" style="width:40%">
                    
    <input type="checkbox" asp-for="FamilyHayFever" class="form-check-input"> Hay Fever                              <input asp-for="FamilyHayFever_" style="width:40%">
                       
    <input type="checkbox" asp-for="FamilyAsthma" class="form-check-input"> Asthma                                 <input asp-for="FamilyAsthma_" style="width:40%">
                    
    <input type="checkbox" asp-for="FamilyEpilepsy" class="form-check-input"> Epilepsy                               <input asp-for="FamilyEpilepsy_" style="width:40%">
            
    <input type="checkbox" asp-for="FamilyGlaucoma" class="form-check-input"> Glaucoma                               <input asp-for="FamilyGlaucoma_" style="width:40%">
                          
    <input type="checkbox" asp-for="FamilySyphilis" class="form-check-input"> Syphilis                               <input asp-for="FamilySyphilis_" style="width:40%">
                       
    <input type="checkbox" asp-for="FamilyNervousDisorders" class="form-check-input"> Nervous Disorders                      <input asp-for="FamilyNervousDisorders_" style="width:40%">
                       
    <input type="checkbox" asp-for="FamilyOther" class="form-check-input"> Other                                  <input asp-for="FamilyOther_" style="width:40%">
    </pre>
</div>
<div class="col-md-12">
    <pre>
    <strong>SECTION III.</strong> FEMALES ONLY:

    <strong>YES</strong>                                                                                                               
    <input type="checkbox" asp-for="HaveYouEverBeenPregnant" class="form-check-input"> Have you ever been pregnant?                           
    <input type="checkbox" asp-for="HaveYouEverHadComplications" class="form-check-input"> Have you ever had complications during a pregnancy?
    <input type="checkbox" asp-for="HaveYouEverHadPainful" class="form-check-input"> Have you ever had painful menstruation?                     
    <input type="checkbox" asp-for="HaveYouEverHadExcessive" class="form-check-input"> Have you ever had excessive menstrual bleeding?
    <input type="checkbox" asp-for="HaveYouEverHadSpotting" class="form-check-input"> Have you ever had spotting or bleeding between periods?
    <input type="checkbox" asp-for="AreYouCurrently" class="form-check-input"> Are you currently on a form of contraceptive? <br />
  
    Age of first menstruation               <input asp-for="AgeOfFirstMenstruation">                      Usual interval between periods     <input asp-for="UsualIntervalBetweenPeriods">
    
    Usual duration of periods               <input asp-for="UsualDurationOfPeriods">                      Date of last period                <input asp-for="DateOfLastPeriod">
    
    Date of last pelvic exam (PAP)          <input asp-for="DateOfLastPelvic">                      Date of last breast exam           <input asp-for="DateOfLastBreastExam">
    </pre>
</div>
<div class="col-md-12">
    <pre>
    <strong>SECTION IV.</strong> 

    <strong>YES</strong>                                                                                                               
    <input type="checkbox" asp-for="AreYouPhysician" class="form-check-input"> Are you on a physician ordered special diet? If YES, type:                        
    <input type="checkbox" asp-for="DoYouSmoke" class="form-check-input"> Do you smoke? If Yes, <input asp-for="DoYouSmoke_PackPerDay"> packs per day for <input asp-for="DoYouSmoke_Year"> years
    </pre>
</div>




