@model KyoS.Web.Models.IntakeAcknoewledgementHippaViewModel
@{
    ViewData["Title"] = "Acknowledgment of Receipt of HIPAA";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />    
</head>
<body>

    <div class="breadcrumbs" id="breadcrumbs">
        <ul class="breadcrumb">
            <li>
                <i class="icon-home"></i>
                @Html.ActionLink("Intake", "IntakeDashboard", "Intakes", new { id = @Model.IdClient })
            </li>
            <li class="active">
                Acknowledgment of Receipt of HIPAA
            </li>
        </ul><!--.breadcrumb-->
    </div>

    <h4>
        Acknowledgment of Receipt of HIPAA “Notice of Privacy Practices”
    </h4>
    <hr />
    <div class="row">       
        <form asp-action="CreateAcknowledgementHippa" asp-route-id="@Model.Id" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="IdClient" />
            <input type="hidden" asp-for="Client_FK" />
            <input type="hidden" asp-for="AdmissionedFor" />   
            <input type="hidden" asp-for="Client.FirstName" />
            <input type="hidden" asp-for="Client.LastName" />
            <partial name="_IntakesAcknowledgementHippa" />
            <div class="form-group col-md-12">
                <input type="submit" value="Save" class="btn btn-primary" />
                <a asp-action="IntakeDashboard" asp-route-id="@Model.Client.Id" class="btn btn-success">Back</a>
            </div>
        </form>       
    </div>
</body>
</html>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    <script src="~/js/jquery.dataTables.min.js"></script>

    <script type="text/javascript">

    </script>
}

