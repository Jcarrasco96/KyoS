@model KyoS.Web.Models.IntakeAcknoewledgementHippaViewModel
@{
    ViewData["Title"] = "Create Intake Acknowledgement of Receipt of HIPPA “Notice of Privacy Practices”";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>
        CREATE INTAKE Acknowledgement of Receipt of HIPPA “Notice of Privacy Practices”
    </title>
</head>
<body>

    <div class="breadcrumbs" id="breadcrumbs">
        <ul class="breadcrumb">
            <li>
                <i class="icon-home"></i>
                @Html.ActionLink("Intakes", "Index", "Intakes")
            </li>
            <li class="active">
                Create Intake Acknowledgement of Receipt of HIPPA “Notice of Privacy Practices”
            </li>
        </ul><!--.breadcrumb-->
    </div>

    <h4>
        Create Intake Acknowledgement of Receipt of HIPPA “Notice of Privacy Practices”
    </h4>
    <hr />
    <div class="row">

        <div>
            <form asp-action="CreateAcknowledgementHippa" asp-route-id="@Model.Id" enctype="multipart/form-data">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <input type="hidden" asp-for="IdClient" />
                <input type="hidden" asp-for="Client_FK" />
                <input type="hidden" asp-for="AdmissionedFor" />               
                <partial name="_IntakesAcknowledgementHippa" />
                <div class="form-group col-md-12">
                    <input type="submit" value="Save" class="btn btn-primary" />
                    <a asp-action="IntakeDashboard" asp-route-id="@Model.Client.Id" class="btn btn-success">Back</a>
                </div>
            </form>
        </div>
    </div>
</body>
</html>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    <script src="~/js/jquery.dataTables.min.js"></script>

    <script type="text/javascript">

    </script>
}

