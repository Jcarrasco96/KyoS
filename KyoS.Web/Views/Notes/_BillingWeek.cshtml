                                                                        <a asp-action="NotDeniedBillFARS" asp-controller="FarsForms" asp-route-idFars="@fars.Id" asp-route-client="0" asp-route-week="@ViewData["idWeek"]" class="btn btn-danger" title="Not Denied Bill"><i class="glyphicon glyphicon-check"></i></a>
                                                                    }
                                                                    else
                                                                    {
                                                                        <a asp-action="DeniedBillTodayFARS" asp-controller="FarsForms" asp-route-idFars="@fars.Id" asp-route-week="@ViewData["idWeek"]" class="btn btn-info" title="Deny Bill"><i class="glyphicon glyphicon-check"></i></a>
                                                                    }

                                                                }

                                                            }
                                                            else
                                                            {
                                                                <p>@fars.PaymentDate.Value.ToShortDateString()</p>
                                                            }
                                                        </td>
                                                        <td align="right">
                                                            @if (ViewData["Billed"].ToString() == "0")
                                                            {
                                                                <a asp-action="BillFARSToday" asp-controller="FarsForms" asp-route-id="@fars.Id" asp-route-week="@ViewData["idWeek"]" class="btn btn-warning" title="Billed today"><i class="glyphicon glyphicon-ok"></i></a>
                                                                <a onclick="showInPopup('@Url.Action("BillFARS", "Notes", new { id = item.Id, abilled = @ViewData["Billed"], week = ViewData["idWeek"], idFars = fars.Id }, Context.Request.Scheme)','Bill this FARS')" class="btn btn-warning" title="Billing FARS"><i class="glyphicon glyphicon-calendar"></i></a>
                                                                <a asp-action="PaymentReceivedTodayFARS" asp-route-week="@ViewData["idWeek"]" asp-route-id="@fars.Id" class="btn btn-success disabled" title="Payment today"><i class="glyphicon glyphicon-ok"></i></a>
                                                                <a onclick="showInPopup('@Url.Action("PaymentReceived", "Notes", new { id = item.Id }, Context.Request.Scheme)','Payment received of this FARS')" class="btn btn-success disabled" title="Payment received"><i class="glyphicon glyphicon-calendar"></i></a>

                                                            }
                                                            @if (ViewData["Billed"].ToString() == "1")
                                                            {
                                                                <a asp-action="NotBillFARS" asp-controller="FarsForms" asp-route-id="@fars.Id" asp-route-week="@ViewData["idWeek"]" asp-route-idFars="@fars.Id" class="btn btn-danger" title="Return not Billed"><i class="glyphicon glyphicon-arrow-left"></i></a>
                                                                <a onclick="showInPopup('@Url.Action("BillFARS", "Notes", new { id = item.Id, abilled = @ViewData["Billed"], week = ViewData["idWeek"], idFars = fars.Id}, Context.Request.Scheme)','Bill this FARS')" class="btn btn-warning" title="Billing FARS"><i class="glyphicon glyphicon-calendar"></i></a>
                                                                @if (fars.PaymentDate == null)
                                                                {
                                                                    <a asp-action="PaymentReceivedTodayFARS" asp-controller="FarsForms" asp-route-week="@ViewData["idWeek"]" asp-route-id="@fars.Id" class="btn btn-success" title="Payment today"><i class="glyphicon glyphicon-ok"></i></a>
                                                                }
                                                                else
                                                                {
                                                                    <a asp-action="NotPaymentReceivedFARS" asp-controller="FarsForms" asp-route-id="@fars.Id" asp-route-week="@ViewData["idWeek"]" asp-route-idFars="@ViewData["idFars"]" class="btn btn-danger" title="Return not Payment"><i class="glyphicon glyphicon-arrow-left"></i></a>
                                                                }
                                                                <a onclick="showInPopup('@Url.Action("PaymentReceivedFARS", "Notes", new { id = item.Id, week = ViewData["idWeek"], idFars = fars.Id}, Context.Request.Scheme)','Payment received of this FARS')" class="btn btn-success" title="Payment received"><i class="glyphicon glyphicon-calendar"></i></a>

                                                            }
                                                        </td>
                                                    </tr>

                                                }
                                            }
                                        </table>
                                    </div>
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Name)
                                    |
                                    @Html.DisplayFor(modelItem => item.Code)
                                </td>
                                <td align="center">
                                    <p>@count</p>
                                </td>
                                <td align="center">
                                    @if (item.Clients_HealthInsurances.Where(n => n.ApprovedDate <= date).Count() > 0)
                                    {
                                        @item.Clients_HealthInsurances.Where(n => n.ApprovedDate <= date).OrderByDescending(n => n.ApprovedDate).ElementAtOrDefault(0).HealthInsurance.Name 
                                        <a> | </a>
                                        @item.Clients_HealthInsurances.Where(n => n.ApprovedDate <= date).OrderByDescending(n => n.ApprovedDate).ElementAtOrDefault(0).MemberId
                                    }
                                </td>
                                <td align="center">
                                  @if (item.Clients_HealthInsurances.Where(n => n.ApprovedDate <= date).Count() > 0)
                                   {
                                       @item.Clients_HealthInsurances.Where(n => n.ApprovedDate <= date).OrderByDescending(n => n.ApprovedDate).ElementAtOrDefault(0).AuthorizationNumber
                                   }
                                </td>
                                <td align="center">
                                    @Html.DisplayFor(modelItem => item.MedicaidID)
                                </td>
                                <td align="center">
                                    @if (item.Clients_Diagnostics.Where(n => n.Principal == true).Count() > 0)
                                    {
                                        @Html.DisplayFor(modelItem => item.Clients_Diagnostics.First(n => n.Principal == true).Diagnostic.Code)
                                    }
                                </td>
                                <td align="right">
                                    @if (item.Workdays_Clients.Where(n => n.BilledDate != null).Count() > 0 || ViewData["Billed"].ToString() == "0")
                                    {
                                        @if (ViewData["Billed"].ToString() == "0")
                                        {
                                            <a asp-action="BillNoteToday" asp-route-id="@item.Id" asp-route-week="@ViewData["idWeek"]" class="btn btn-warning" title="Billed today"><i class="glyphicon glyphicon-ok"></i></a>
                                            <a onclick="showInPopup('@Url.Action("BillNote", "Notes", new { id = item.Id, week = @ViewData["idWeek"], abilled = @ViewData["Billed"] }, Context.Request.Scheme)','Bill this note')" class="btn btn-warning" title="Billing note"><i class="glyphicon glyphicon-calendar"></i></a>
                                            <a asp-action="PaymentReceivedToday" asp-route-id="@item.Id" asp-route-week="@ViewData["idWeek"]" class="btn btn-success disabled" title="Payment today"><i class="glyphicon glyphicon-ok"></i></a>
                                            <a onclick="showInPopup('@Url.Action("PaymentReceived", "Notes", new { id = item.Id, week = @ViewData["idWeek"] }, Context.Request.Scheme)','Payment received of this note')" class="btn btn-success disabled" title="Payment received"><i class="glyphicon glyphicon-calendar"></i></a>
                                        }
                                        else
                                        {
                                            <a asp-action="NotBill" asp-route-id="@item.Id" asp-route-week="@ViewData["idWeek"]" class="btn btn-danger" title="Return not Billed"><i class="glyphicon glyphicon-arrow-left"></i></a>
                                            <a onclick="showInPopup('@Url.Action("BillNote", "Notes", new { id = item.Id, week = @ViewData["idWeek"], abilled = @ViewData["Billed"] }, Context.Request.Scheme)','Bill this note')" class="btn btn-warning " title="Billing note"><i class="glyphicon glyphicon-calendar"></i></a>
                                            @if (item.Workdays_Clients.Where(n => n.PaymentDate == null).Count() > 0)
                                            {
                                                <a asp-action="PaymentReceivedToday" asp-route-id="@item.Id" asp-route-week="@ViewData["idWeek"]" class="btn btn-success" title="Payment today"><i class="glyphicon glyphicon-ok"></i></a>
                                            }
                                            else
                                            {
                                                <a asp-action="NotPaymentReceived" asp-route-id="@item.Id" asp-route-week="@ViewData["idWeek"]" class="btn btn-danger" title="Return not Payment"><i class="glyphicon glyphicon-arrow-left"></i></a>
                                            }
                                            <a onclick="showInPopup('@Url.Action("PaymentReceived", "Notes", new { id = item.Id, week = @ViewData["idWeek"] }, Context.Request.Scheme)','Payment received of this note')" class="btn btn-success" title="Payment received"><i class="glyphicon glyphicon-calendar"></i></a>
                                        }
                                    }
                                    else
                                    {
                                        <a asp-action="BillNoteToday" asp-route-id="@item.Id" asp-route-week="@ViewData["idWeek"]" class="btn btn-danger disabled" title="Billed today"><i class="glyphicon glyphicon-arrow-left"></i></a>
                                        <a onclick="showInPopup('@Url.Action("BillNote", "Notes", new { id = item.Id, week = @ViewData["idWeek"], abilled = @ViewData["Billed"] }, Context.Request.Scheme)','Bill this note')" class="btn btn-warning disabled" title="Billing note"><i class="glyphicon glyphicon-calendar"></i></a>
                                        <a asp-action="PaymentReceivedToday" asp-route-id="@item.Id" asp-route-week="@ViewData["idWeek"]" class="btn btn-danger disabled" title="Payment today"><i class="glyphicon glyphicon-arrow-left"></i></a>
                                        <a onclick="showInPopup('@Url.Action("PaymentReceived", "Notes", new { id = item.Id, week = @ViewData["idWeek"] }, Context.Request.Scheme)','Payment received of this note')" class="btn btn-success disabled" title="Payment received"><i class="glyphicon glyphicon-calendar"></i></a>
                                    }
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
