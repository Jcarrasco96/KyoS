@model IEnumerable<KyoS.Common.Helpers.AuditOverlapin>
@{
    Layout = null;
}
<div class="row">
    <div class="col-md-12">
        <form asp-action="AuditOverlapin">
            <div class="col-md-12">
                @if (Model != null)
                {
                    <div class="col-md-12">
                        <div class="form-group">
                            <div class="panel panel-default">
                                <div class="panel-heading">
                                    <h3 class="panel-title">Audit (Clients with Overlapin)</h3>
                                </div>
                                <div class="panel-body">
                                    <table class="table table-hover table-responsive table-striped" id="MyTableDiag">
                                        <thead>
                                            <tr>
                                                <th class="col-md-5">
                                                    Name
                                                </th>
                                                <th class="col-md-2" style="text-align:center;">
                                                    Date
                                                </th>
                                                <th class="col-md-2" style="text-align:center;">
                                                    Service
                                                </th>
                                                <th class="col-md-3" style="text-align:center;">
                                                    Schedule
                                                </th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @foreach (var item in Model)
                                            {
                                                <tr>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.Name)
                                                    </td>
                                                    <td align="center">
                                                        @Html.DisplayFor(modelItem => item.Date.ToShortDateString())
                                                    </td>
                                                    <td align="center">
                                                        @Html.DisplayFor(modelItem => item.Services)
                                                    </td>
                                                    <td align="center">
                                                        @Html.DisplayFor(modelItem => item.Schedule)
                                                    </td>
                                                </tr>
                                            }
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                }
            </div>
            <div class="col-md-offset-10">
                &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<button type="button" class="btn btn-success" data-dismiss="modal">Close</button>
            </div>
        </form>
    </div>
</div>



