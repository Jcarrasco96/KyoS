@model KyoS.Web.Models.TCMFarsFormViewModel
@{
    ViewData["Title"] = "Create TCM Fars Form";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
</head>
<body>

    <div class="breadcrumbs" id="breadcrumbs">
        <ul class="breadcrumb">
            <li>
                <i class="icon-home"></i>
                @if (ViewData["origi"].ToString() == "0")
                {
                    @Html.ActionLink("TCM Fars Forms", "Index", "TCMFarsForms", new { origin = @ViewData["origi"] })
                }
                @if (ViewData["origi"].ToString() == "1")
                {
                    @Html.ActionLink("TCM Fars Forms", "Index", "TCMFarsForms", new { idTCMClient = Model.TCMClient.Id, origin = @ViewData["origi"] })
                }

            </li>
            <li class="active">Create TCM Fars Form</li>
        </ul><!--.breadcrumb-->
    </div>

    <h4>Create Fars Form</h4>
    <hr />
    <div class="row">
        <form asp-action="Create" asp-route-id="@Model.Id" asp-route-origi="@ViewData["origi"]" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="IdTCMClient" />
            <input type="hidden" asp-for="AdmissionedFor" />
            <input type="hidden" asp-for="Status" />
            <input type="hidden" asp-for="CreatedBy" />
            <input type="hidden" asp-for="CreatedOn" />
            <input type="hidden" asp-for="LastModifiedBy" />
            <input type="hidden" asp-for="LastModifiedOn" />
            <input type="hidden" asp-for="TCMClient.CaseNumber" />
            <partial name="_TCMFarsForms" />
            <div class="form-group col-md-12">
                <input type="submit" value="Create" class="btn btn-primary" />
                @if (ViewData["origi"].ToString() == "0")
                {
                    <a asp-action="Index" asp-route-origin="@ViewData["origi"]" class="btn btn-success">Back</a>
                }
                @if (ViewData["origi"].ToString() == "1")
                {
                    <a asp-action="Index" asp-route-idTCMClient="@Model.TCMClient.Id" asp-route-origin="@ViewData["origi"]" class="btn btn-success">Back</a>
                }
            </div>
        </form>
    </div>
</body>
</html>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    <script src="~/js/jquery.dataTables.min.js"></script>

    <script type="text/javascript">

    </script>
}

