@model KyoS.Web.Models.TCMTransferViewModel
@{
    Layout = null;
}

<div class="row">
    <form asp-action="SelectTCMClientForTransfer">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="col-md-12">
            <div class="col-md-12">
                <p>This Transfer is pending due to:</p>
            </div>
        </div>
        <div class="col-md-1">
            &nbsp;
        </div>
        <div class="col-md-11">
            <input asp-for="TCMSupervisorAccept" disabled /><i> Supervisor signature</i><br />
            <input asp-for="TCMAssignedFromAccept" disabled /><i> Case Manager signature (from)</i><br />
            <input asp-for="TCMAssignedToAccept" disabled /><i> Case Manager signature (to)</i><br />
        </div>
        <div class="col-md-12">
            <hr />
        </div>
        <div class="col-md-12">
            <div class="col-md-offset-10">
                <button type="button" class="btn btn-success" data-dismiss="modal">Back</button>
            </div>
        </div>
    </form>
</div>

