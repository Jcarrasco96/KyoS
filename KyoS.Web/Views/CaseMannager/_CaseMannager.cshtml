@model KyoS.Web.Models.CaseMannagerViewModel

<div class="col-md-3 form-group">
    <label asp-for="Name" class="control-label"></label>
    <input asp-for="Name" class="form-control" />
    <span asp-validation-for="Name" class="text-danger"></span>
</div>
<div class="col-md-3 form-group">
    <label asp-for="IdClinic" class="control-label"></label>
    <select asp-for="IdClinic" asp-items="Model.Clinics" class="form-control"></select>
    <span asp-validation-for="IdClinic" class="text-danger"></span>
</div>
<div class="col-md-2 form-group">
    <label asp-for="ProviderNumber" class="control-label"></label>
    <input asp-for="ProviderNumber" class="form-control" />
    <span asp-validation-for="ProviderNumber" class="text-danger"></span>
</div>
<div class="col-md-2 form-group">
    <label asp-for="Credentials" class="control-label"></label>
    <input asp-for="Credentials" class="form-control" />
    <span asp-validation-for="Credentials" class="text-danger"></span>
</div>
<div class="col-md-2 form-group">
    <label asp-for="IdStatus" class="control-label"></label>
    <select asp-for="IdStatus" asp-items="Model.StatusList" class="form-control"></select>
    <span asp-validation-for="IdStatus" class="text-danger"></span>
</div>
<div class="col-md-3 form-group">
    <label asp-for="IdUser" class="control-label"></label>
    <select asp-for="IdUser" asp-items="Model.UserList" class="form-control"></select>
    <span asp-validation-for="IdUser" class="text-danger"></span>
</div>
<div class="col-md-3 form-group">
    <label asp-for="SignatureFile" class="control-label"></label>
    <input asp-for="SignatureFile" type="file" class="form-control" />
    <span asp-validation-for="SignatureFile" class="text-danger"></span>
</div>
<div class="col-md-2 form-group">
    <label asp-for="Phone" class="control-label"></label>
    <input asp-for="Phone" class="form-control" />
    <span asp-validation-for="Phone" class="text-danger"></span>
</div>
<div class="col-md-2 form-group">
    <label asp-for="Email" class="control-label"></label>
    <input asp-for="Email" class="form-control" />
    <span asp-validation-for="Email" class="text-danger"></span>
</div>
<div class="col-md-2 form-group">
    <label asp-for="Money" class="control-label">Salary / Hour</label>
    <input asp-for="Money" class="form-control" />
    <span asp-validation-for="Money" class="text-danger"></span>
</div>
