@model KyoS.Web.Models.TCMIntakeCoordinationCareViewModel
@{
    ViewData["Title"] = "Coordination of Care";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
</head>
<body>

    <div class="breadcrumbs" id="breadcrumbs">
        <ul class="breadcrumb">
            <li>
                <i class="icon-home"></i>
                @if (User.IsInRole("Manager"))
                {
                    @Html.ActionLink("TCM Intake Section 3", "TCMIntakeSectionDashboardReadOnly", "TCMIntakes", new { id = @Model.IdTCMClient, section = 3 })
                }
            </li>
            <li class="active">
                Coordination of Care
            </li>
        </ul><!--.breadcrumb-->
    </div>

    <h4>
        Coordination of Care
    </h4>
    <hr />
    <div class="row">
        <form asp-action="TCMCoordinationCareReadOnly" asp-route-id="@Model.Id" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="IdTCMClient" />
            <input type="hidden" asp-for="TcmClient_FK" />
            <input type="hidden" asp-for="CreatedOn" />
            <input type="hidden" asp-for="CreatedBy" />
            <input type="hidden" asp-for="AdmissionedFor" />
            <partial name="_TCMIntakeCoordinationReadOnly" />
            <div class="form-group col-md-12">
                @if (User.IsInRole("Manager"))
                {
                    <a asp-action="TCMIntakeSectionDashboardReadOnly" asp-route-id="@Model.IdTCMClient" asp-route-section="3" class="btn btn-success">Back</a>
                }
            </div>
        </form>
    </div>
</body>
</html>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    <script src="~/js/jquery.dataTables.min.js"></script>

    <script type="text/javascript">

    </script>
}

