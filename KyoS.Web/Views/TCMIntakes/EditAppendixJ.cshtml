@model KyoS.Web.Models.TCMIntakeAppendixJViewModel
@{
    ViewData["Title"] = "TCM AppendixJ";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
</head>
<body>

    <div class="breadcrumbs" id="breadcrumbs">
        <ul class="breadcrumb">
            <li>
                <i class="icon-home"></i>
                @if (ViewData["origi"].ToString() == "0")
                {
                    @Html.ActionLink("TCM AppendixJ List", "ListAppendixJForTCMClient", "TCMIntakes", new { id = Model.IdTCMClient, origi = ViewData["origi"] })
                }
                else
                {
                    @Html.ActionLink("Update Appendix J", "UpdateAppendixJ", "TCMIntakes")
                }
                
            </li>
            <li class="active">TCM AppendixJ</li>
        </ul><!--.breadcrumb-->
    </div>

    <h4>TCM AppendixJ</h4>
    <hr />
    <div class="row">
        <form asp-action="EditAppendixJ" asp-route-id="@Model.Id" asp-route-origi="@ViewData["origi"]" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="IdTCMClient" />
            <input type="hidden" asp-for="TcmClient_FK" />
            <input type="hidden" asp-for="AdmissionedFor" />
            <input type="hidden" asp-for="CreatedOn" />
            <input type="hidden" asp-for="CreatedBy" />
            <input type="hidden" asp-for="DateExpired" />
            <input type="hidden" asp-for="Active" />
            <input type="hidden" asp-for="Approved" />
            <input type="hidden" asp-for="TcmClient.Client.FirstName" />
            <input type="hidden" asp-for="TcmClient.Client.LastName" />
            <partial name="_TCMIntakeAppendixJReadOnly" />
            <div class="form-group col-md-12">
                <input type="submit" value="Save" class="btn btn-primary" />
                @if (ViewData["origi"].ToString() == "0")
                {
                    <a asp-action="ListAppendixJForTCMClient" asp-route-id="@Model.IdTCMClient" asp-route-origi="@ViewData["origi"]" class="btn btn-success">Back</a>
                }
                else
                {
                    <a asp-action="UpdateAppendixJ" class="btn btn-success">Back</a>
                }
                
            </div>
        </form>
    </div>
</body>
</html>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    <script src="~/js/jquery.dataTables.min.js"></script>

    <script type="text/javascript">

    </script>
}

