@model KyoS.Web.Models.TCMIntakeAppendixIViewModel
@{
    ViewData["Title"] = "TCM Appendix I";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
</head>
<body>

    <div class="breadcrumbs" id="breadcrumbs">
        <ul class="breadcrumb">
            <li>
                <i class="icon-home"></i>
                @Html.ActionLink("TCM Client History", "TCMCaseHistory", "TCMClients", new { id = @Model.TcmClient_FK })
            </li>
            <li class="active">TCM Appendix I</li>
        </ul><!--.breadcrumb-->
    </div>

    <h4>TCM Appendix I</h4>
    <hr />
    <div class="row">
        <form asp-action="EditAppendixIReadOnly" asp-route-id="@Model.Id" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="IdTCMClient" />
            <input type="hidden" asp-for="TcmClient_FK" />
            <input type="hidden" asp-for="AdmissionedFor" />
            <input type="hidden" asp-for="CreatedOn" />
            <input type="hidden" asp-for="CreatedBy" />
            <input type="hidden" asp-for="TcmClient.Client.FirstName" />
            <input type="hidden" asp-for="TcmClient.Client.LastName" />
            <partial name="_TCMIntakeAppendixIReadOnly" />
            <div class="form-group col-md-12">
                @if (ViewData["origi"].ToString() == "0")
                {
                    <input type="submit" value="Update" class="btn btn-primary" />
                }
                <a asp-action="TCMCaseHistory" asp-controller="TCMClients" asp-route-id="@Model.TcmClient_FK" class="btn btn-success">Back</a>
            </div>
        </form>
    </div>
</body>
</html>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    <script src="~/js/jquery.dataTables.min.js"></script>

    <script type="text/javascript">

    </script>
}

