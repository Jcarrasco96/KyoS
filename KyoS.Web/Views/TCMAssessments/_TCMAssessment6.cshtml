        School: <input asp-for="SchoolName" style="width:25%" />      District:        <input asp-for="SchoolDistrict" style="width:20%" />   Grade: <input asp-for="SchoolGrade" style="width:20%" />
        Address <input asp-for="SchoolAddress" style="width:25%" />      City, State, Zip <input asp-for="SchoolCityState" style="width:20%" />
