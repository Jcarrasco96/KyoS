                    </div>
                </div>
                <div class="">
                    <div id="view-AssessmentMedicalProblem">
                        @await Html.PartialAsync("_ViewMedicalProblem", Model.MedicalProblemList.ToList())
                    </div>
                </div>
            </div>
            <partial name="_TCMAssessment5" />
            <div class="col-md-push-12">
                <div class="">
                    <div class="col-md-12 form-group">
                        <a onclick="showInPopup('@Url.Action("CreateSurgeryModal", "TCMAssessments",new { idAssessment = @Model.Id}, Context.Request.Scheme)','Create TCM Assessment')" class="btn btn-success" title="Create TCM Assessment"><i class="glyphicon glyphicon-plus"></i> Add Surgery</a>
                    </div>
                </div>
                <div class="">
                    <div id="view-AssessmentSurgery">
                        @await Html.PartialAsync("_ViewSurgery", Model.SurgeryList.ToList())
                    </div>
                </div>
            </div>
            <partial name="_TCMAssessment6" />
            <div class="form-group col-md-12">
                <input type="submit" value="Save" class="btn btn-primary" />
                @if (Model.Approved == 0)
                {
                    <a asp-action="FinishEditing" asp-route-id="@Model.Id" asp-route-section="4" asp-controller="TCMAssessments" class="btn btn-warning">Sign / Finish Edition</a>
                }
                
                @if (ViewData["origi"].ToString() == "0")
                {
                    <a asp-action="TCMIntakeSectionDashboard" asp-route-id="@Model.TcmClient.Id" asp-route-section="4" asp-controller="TCMIntakes" class="btn btn-success">Back</a>
                }
                @if (ViewData["origi"].ToString() == "1")
                {
                    <a asp-action="MessagesOfAssessment" asp-controller="TCMMessages" class="btn btn-success">Back</a>
                }
                @if (ViewData["origi"].ToString() == "3")
                {
