                }
                @if (ViewData["origi"].ToString() == "5")
                {
                    <a asp-action="UpdateAssessment" asp-controller="TCMAssessments"  class="btn btn-success">Back</a>
                }
            </div>
        </form>
    </div>
</body>
</html>

