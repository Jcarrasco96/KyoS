                    </div>
                </div>
                <div class="">
                    <div id="view-AssessmentHospital">
                        @await Html.PartialAsync("_ViewHospital", Model.HospitalList.ToList())
                    </div>
                </div>
            </div>
            <partial name="_TCMAssessment3" />
            <div class="col-md-push-12">
                <div class="">
