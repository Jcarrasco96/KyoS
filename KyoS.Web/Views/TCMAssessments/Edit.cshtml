                    </div>
                </div>
                <div class="">
                    <div id="view-AssessmentDrug">
                        @await Html.PartialAsync("_ViewDrug", Model.DrugList.ToList())
                    </div>
                </div>
            </div>
            <partial name="_TCMAssessment4" />
            <div class="col-md-push-12">
                <div class="">
                    <div class="col-md-12 form-group">
