@model KyoS.Web.Models.TCMAssessmentViewModel
@{
    ViewData["Title"] = "Edit TCM Assessment";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
</head>
<body>

    <div class="breadcrumbs" id="breadcrumbs">
        <ul class="breadcrumb">
            <li>
                <i class="icon-home"></i>
                @if (ViewData["origi"].ToString() == "0")
                {
                    @Html.ActionLink("TCM Intake Section 4", "TCMIntakeSectionDashboard", "TCMIntakes", new { id = @Model.TcmClient_FK, section = 4 })
                }
                @if (ViewData["origi"].ToString() == "1")
                {
                    @Html.ActionLink("TCM Assessment Messages", "MessagesOfAssessment", "TCMMessages")
                }
                @if (ViewData["origi"].ToString() == "3")
                {
                    @Html.ActionLink("Dashboard", "Index", "Desktop")
                }
                @if (ViewData["origi"].ToString() == "4")
                {
                    @Html.ActionLink("Assessments in edition", "TCMAssessmentApproved", "TCMAssessments")
                }
                @if (ViewData["origi"].ToString() == "5")
                {
                    @Html.ActionLink("Assessments Update", "UpdateAssessment", "TCMAssessments")
                }
            </li>
            <li class="active">Edit TCM Assessment</li>
        </ul><!--.breadcrumb-->
    </div>

    <h4>Edit TCM Assessment</h4>
    <hr />
    <div class="row">
        <form asp-action="Edit" asp-route-id="@Model.Id" asp-route-origi="@ViewData["origi"]" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="TcmClient_FK" />
            <input type="hidden" asp-for="CreatedBy" />
            <input type="hidden" asp-for="CreatedOn" />
            <input type="hidden" asp-for="Approved" />
            <partial name="_TCMAssessment" />
            <div class="col-md-push-12">
                <div class="">
                    <div class="col-md-12 form-group">
                        <a onclick="showInPopup('@Url.Action("AddReferred", "TCMAssessments",new { id = @Model.TcmClient.Client.Id}, Context.Request.Scheme)','Create Referred')" class="btn btn-success" title="Create Referred"><i class="glyphicon glyphicon-plus"></i> Add Referred</a>
                    </div>
                </div>
                <div class="">
                    <div id="view-referreds">
                        @await Html.PartialAsync("_ViewReferred", @Model.TcmClient.Client.Client_Referred.ToList())
                    </div>
                </div>
            </div>
            <partial name="_TCMAssessment0" />
            <div class="col-md-push-12">
                <div class="">
                    <div class="col-md-12 form-group">
                        <a onclick="showInPopup('@Url.Action("CreateIndividualAgencyModal", "TCMAssessments",new { idAssessment = @Model.Id}, Context.Request.Scheme)','Add Individual Agency')" class="btn btn-success" title="Create TCM Assessment"><i class="glyphicon glyphicon-plus"></i> Add Individual Agency</a>
                    </div>
                </div>
                <div class="">
                    <div id="view-AssessmentIndividual">
                        @await Html.PartialAsync("_ViewIndividualAgency", Model.IndividualAgencyList.ToList())
                    </div>
                </div>
            </div>
            <partial name="_TCMAssessment1" />
            <div class="col-md-push-12">
                <div class="">
                    <div class="col-md-12 form-group">
