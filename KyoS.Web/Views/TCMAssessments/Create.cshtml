@model KyoS.Web.Models.TCMAssessmentViewModel
@{
    ViewData["Title"] = "Create Case Management Assessment";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Create Case Management Assessment</title>
</head>
<body>

    <div class="breadcrumbs" id="breadcrumbs">
        <ul class="breadcrumb">
            <li>
                <i class="icon-home"></i>
                @Html.ActionLink("TCM Intake Section 4", "TCMIntakeSectionDashboard", "TCMAssessments", new { id = @Model.TcmClient.Id, section = 4 })
            </li>
            <li class="active">Create Case Management Assessment</li>
        </ul><!--.breadcrumb-->
    </div>

    <h4>Create Case Management Assessment</h4>
    <hr />
    <div class="row">

        <div>
            <form asp-action="Create" asp-route-id="@Model.Id" enctype="multipart/form-data">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <input type="hidden" asp-for="TcmClient_FK" />
                <partial name="_TCMAssessment" />
                <div class="form-group col-md-12">
                    <input type="submit" value="Create" class="btn btn-primary" />
                    <a asp-action="Index" class="btn btn-success">Back</a>
                </div>
            </form>
        </div>
    </div>
</body>
</html>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    <script src="~/js/jquery.dataTables.min.js"></script>

    <script type="text/javascript">

    </script>
}


