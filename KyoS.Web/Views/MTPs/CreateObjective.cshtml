@model KyoS.Web.Models.ObjectiveViewModel

@{
    ViewData["Title"] = "Create";
}

@*<link rel="stylesheet" href="~/css/style.css">*@
<link rel="stylesheet" href="~/css/prism.css">
<link rel="stylesheet" href="~/css/chosen.css">

<h4>Create objective</h4>
<hr />
<div class="row">
    <div>
        <form asp-action="CreateObjective">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <input type="hidden" asp-for="IdGoal" />
            <partial name="_Objectives" />
            <div class="form-group col-md-12"></div>
            <div class="form-group col-md-12">
                <input type="submit" value="Save" class="btn btn-primary" />
                <a asp-action="UpdateObjectives" class="btn btn-success" asp-route-id="@Model.Goal.Id">Back</a>
            </div>
        </form>
    </div>
</div>



@section Scripts {
    <script src="~/js/chosen.jquery.js" type="text/javascript"></script>
    <script src="~/js/prism.js" type="text/javascript" charset="utf-8"></script>
    <script src="~/js/init.js" type="text/javascript" charset="utf-8"></script>    
}




