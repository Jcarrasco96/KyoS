      <TextObject Name="Text327" Top="59.7" Width="774.9" Height="56.7" Text="The Mini Mental State Examination (MMSE) is a tool that can be used to systematically and thoroughly assess mental status. It is an 11-question measure that tests five areas of cognitive function: orientation, registration, attention and calculation, recall, and language. The maximum score is 30. A score of 23 or lower is indicative of cognitive impairment." HorzAlign="Justify" Font="Arial, 10pt" TextRenderType="HtmlTags"/>
      <TextObject Name="Text328" Left="1" Top="127.85" Width="75.6" Height="18.9" Text="Maximum" HorzAlign="Center" Font="Arial, 10pt" TextRenderType="HtmlTags"/>
      <TextObject Name="Text329" Left="100.5" Top="127.85" Width="75.6" Height="18.9" Text="Score" HorzAlign="Center" Font="Arial, 10pt" TextRenderType="HtmlTags"/>
      <TextObject Name="Text330" Left="207.9" Top="141.75" Width="94.5" Height="18.9" Text="Orientation" Font="Arial, 10pt, style=Bold" TextRenderType="HtmlTags"/>
      <TextObject Name="Text331" Left="207.9" Top="160.65" Width="519.75" Height="18.9" Text="What is the (year) (season) (date) (day) (month)?" Font="Arial, 10pt" TextRenderType="HtmlTags"/>
      <TextObject Name="Text332" Left="207.9" Top="179.55" Width="519.75" Height="18.9" Text="Where are we (state) (country) (town) (hospital) (home) (office) (floor)?" Font="Arial, 10pt" TextRenderType="HtmlTags"/>
      <TextObject Name="Text333" Left="28.35" Top="160.65" Width="18.9" Height="18.9" Text="5" HorzAlign="Center" Font="Arial, 10pt" TextRenderType="HtmlTags"/>
      <TextObject Name="Text334" Left="28.35" Top="179.55" Width="18.9" Height="18.9" Text="5" HorzAlign="Center" Font="Arial, 10pt" TextRenderType="HtmlTags"/>
      <TextObject Name="Text335" Left="125.85" Top="160.65" Width="26.46" Height="18.9" Text="([TCMIntakeMiniMental.OrientationWhat])" HorzAlign="Center" Font="Arial, 10pt" TextRenderType="HtmlTags"/>
      <TextObject Name="Text336" Left="125.85" Top="179.55" Width="26.46" Height="18.9" Text="([TCMIntakeMiniMental.OrientationWhere])" HorzAlign="Center" Font="Arial, 10pt" TextRenderType="HtmlTags"/>
      <TextObject Name="Text337" Left="207.9" Top="207.9" Width="103.95" Height="18.9" Text="Registration" Font="Arial, 10pt, style=Bold" TextRenderType="HtmlTags"/>
      <TextObject Name="Text338" Left="207.9" Top="226.8" Width="557.55" Height="66.15" Text="Name 3 objects: 1 second to say each. Then ask the patient all 3 after you have said them. Give 1 point for each correct answer.&#13;&#10;Then repeat them until he/she learns all 3. Count trials and record.&#13;&#10;Trials &lt;u&gt;[TCMIntakeMiniMental.Trials]&lt;/u&gt;" Font="Arial, 10pt" TextRenderType="HtmlTags"/>
      <TextObject Name="Text339" Left="28.35" Top="226.8" Width="18.9" Height="18.9" Text="3" HorzAlign="Center" Font="Arial, 10pt" TextRenderType="HtmlTags"/>
      <TextObject Name="Text340" Left="125.85" Top="226.8" Width="26.46" Height="18.9" Text="([TCMIntakeMiniMental.RegistrationName])" HorzAlign="Center" Font="Arial, 10pt" TextRenderType="HtmlTags"/>
      <TextObject Name="Text341" Left="207.9" Top="301.4" Width="292.95" Height="18.9" Text="Attention and Calculation" Font="Arial, 10pt, style=Bold" TextRenderType="HtmlTags"/>
      <TextObject Name="Text342" Left="207.9" Top="320.3" Width="557.55" Height="37.8" Text="Serial 7’s. 1 point for each correct answer. Stop after 5 answers.&#13;&#10;Alternatively spell “world” backward. Give 1 point for each letter spelled correctly." Font="Arial, 10pt" TextRenderType="HtmlTags"/>
      <TextObject Name="Text343" Left="28.35" Top="320.3" Width="18.9" Height="18.9" Text="5" HorzAlign="Center" Font="Arial, 10pt" TextRenderType="HtmlTags"/>
      <TextObject Name="Text344" Left="125.85" Top="320.3" Width="26.46" Height="18.9" Text="([TCMIntakeMiniMental.Attention])" HorzAlign="Center" Font="Arial, 10pt" TextRenderType="HtmlTags"/>
      <TextObject Name="Text345" Left="207.9" Top="367.55" Width="292.95" Height="18.9" Text="Recall" Font="Arial, 10pt, style=Bold" TextRenderType="HtmlTags"/>
      <TextObject Name="Text346" Left="207.9" Top="386.45" Width="557.55" Height="18.9" Text="Ask for the 3 objects repeated above. Give 1 point for each correct answer." Font="Arial, 10pt" TextRenderType="HtmlTags"/>
      <TextObject Name="Text347" Left="28.35" Top="386.45" Width="18.9" Height="18.9" Text="3" HorzAlign="Center" Font="Arial, 10pt" TextRenderType="HtmlTags"/>
      <TextObject Name="Text348" Left="125.85" Top="386.45" Width="26.46" Height="18.9" Text="([TCMIntakeMiniMental.Recall])" HorzAlign="Center" Font="Arial, 10pt" TextRenderType="HtmlTags"/>
      <TextObject Name="Text349" Left="207.9" Top="413.8" Width="94.5" Height="18.9" Text="Language" Font="Arial, 10pt, style=Bold" TextRenderType="HtmlTags"/>
      <TextObject Name="Text350" Left="207.9" Top="432.7" Width="519.75" Height="18.9" Text="Name a pencil and watch." Font="Arial, 10pt" TextRenderType="HtmlTags"/>
      <TextObject Name="Text351" Left="207.9" Top="451.6" Width="519.75" Height="18.9" Text="Repeat the following: “No ifs, ands, or buts”" Font="Arial, 10pt" TextRenderType="HtmlTags"/>
      <TextObject Name="Text352" Left="28.35" Top="432.7" Width="18.9" Height="18.9" Text="2" HorzAlign="Center" Font="Arial, 10pt" TextRenderType="HtmlTags"/>
      <TextObject Name="Text353" Left="28.35" Top="451.6" Width="18.9" Height="18.9" Text="1" HorzAlign="Center" Font="Arial, 10pt" TextRenderType="HtmlTags"/>
